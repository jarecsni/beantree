{
	"name": "beantree",
	"version": "0.0.2",
	"description": "A revolutionary frontend framework based on beans and a tree.",
	"author": "Johnny Jarecsni <johnny.jarecsni@icloud.com>",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"graph",
		"render",
		"framework"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jarecsni/beantree"
	},
	"bugs": {
		"url": "https://github.com/jarecsni/beantree/issues"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"test": "playwright test",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "^1.15.9",
		"@sveltejs/package": "^2.0.2",
		"@testing-library/svelte": "^3.2.2",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"happy-dom": "^9.9.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vitest": "^0.30.1",
		"vitest-localstorage-mock": "^0.0.1"
	},
	"type": "module",
	"dependencies": {
		"@smui-extra/accordion": "^7.0.0-beta.8",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/data-table": "^7.0.0-beta.8",
		"@smui/dialog": "^7.0.0-beta.8",
		"@smui/list": "^7.0.0-beta.4",
		"@smui/menu": "^7.0.0-beta.4",
		"@smui/menu-surface": "^7.0.0-beta.4",
		"@smui/switch": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8"
	}
}
