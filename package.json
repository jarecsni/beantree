		"@smui/menu": "^7.0.0-beta.9",
		"@smui/menu-surface": "^7.0.0-beta.9",
		"@smui/switch": "^7.0.0-beta.9",
		"@smui/textfield": "^7.0.0-beta.9",
		"firebase": "^9.23.0",
		"svelte": "^4.0.0"
