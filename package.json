	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"graph",
		"render",
		"framework"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jarecsni/beantree"
	},
	"bugs": {
		"url": "https://github.com/jarecsni/beantree/issues"
	},
	"scripts": {
		"dev": "vite dev",
