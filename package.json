		"@smui/switch": "^7.0.0-beta.14",
		"@smui/textfield": "^7.0.0-beta.14",
		"firebase": "^8.6.8",
		"svelte": "^4.0.5",
		"ts-bus": "^3.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
